{
  "version": 2,
  "builds": [
    {
      "src": "asgi.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/asgi.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "backend.settings",
    "PYTHONUNBUFFERED": "1"
  },
  "functions": {
    "api": {
      "runtime": "vercel.python",
      "memory": 1024,
      "includeFiles": "asgi.py, requirements.txt",
      "env": {
        "DJANGO_SETTINGS_MODULE": "backend.settings"
      }
    }
  },
  "build": {
    "env": {
      "DJANGO_SETTINGS_MODULE": "backend.settings"
    }
  },
  "routes": [],
  "rewrites": [],
  "redirects": []
}
