{
  "version": 2,
  "builds": [
    {
      "src": "asgi.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/asgi.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "backend.settings",
    "PYTHONUNBUFFERED": "1"
  },
  "build": {
    "env": {
      "DJANGO_SETTINGS_MODULE": "backend.settings"
    },
    "postinstall": "pip install -r requirements.txt"
  }
}
